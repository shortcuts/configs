# git
abbr -a gs   git status -sb
abbr -a ga   git add .
abbr -a gc   git commit -m
abbr -a gca  git commit --amend
abbr -a gcl  git clone
abbr -a gl   git log
abbr -a gd   git diff
abbr -a gf   git fetch origin
abbr -a glc  git log --oneline HEAD ^main

