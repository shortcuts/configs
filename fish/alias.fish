# git
abbr -a gs   git status -sb
abbr -a ga   git add .
abbr -a gc   git commit -m
abbr -a gca  git commit --amend
abbr -a gcl  git clone
abbr -a gp   git push
abbr -a gpf  git push -f origin
abbr -a gl   git log
abbr -a gd   git diff
abbr -a gf   git fetch origin

abbr -a gw   git worktree list
abbr -a gwa  git worktree add

# misc
alias reload='exec fish'
alias cat='bat'
alias vim='nvim'
alias v='nvim .'
